{
    "Task_Finish": {
        "type": "function",
        "function": {
            "name": "Task_Finish",
            "description": "通知用户任务已完成，用于通知用户任务已完成或执行成功。"
        }
    },
    "Read_File": {
        "type": "function",
        "function": {
            "name": "Read_File",
            "description": "读取文件内容。用于查看文件内容、分析日志或读取配置文件。",
            "parameters": {
                "type": "object",
                "properties": {
                    "file_path": {
                        "type": "string",
                        "description": "文件路径，必须使用绝对路径（以/开头）。"
                    },
                    "start_line": {
                        "type": "integer",
                        "description": "（可选）起始行号，从1开始。"
                    },
                    "end_line": {
                        "type": "integer",
                        "description": "（可选）结束行号，从1开始。"
                    }
                },
                "required": ["file_path"]
            }
        }
    },
    "Write_File": {
        "type": "function",
        "function": {
            "name": "Write_File",
            "description": "覆盖或追加内容到文件。用于创建新文件、追加内容或修改现有文件。",
            "parameters": {
                "type": "object",
                "properties": {
                    "file_path": {
                        "type": "string",
                        "description": "文件路径，必须使用绝对路径（以/开头）。"
                    },
                    "content": {
                        "type": "string",
                        "description": "要写入的文本内容"
                    },
                    "append": {
                        "type": "boolean",
                        "description": "（可选）是否启用追加模式，默认为false。"
                    },
                    "front_newline": {
                        "type": "boolean",
                        "description": "（可选）是否在写入内容前面添加换行符，默认为false。"
                    },
                    "end_newline": {
                        "type": "boolean",
                        "description": "（可选）是否在写入内容后面添加换行符，默认为false。"
                    }
                },
                "required": ["file_path", "content"]
            }
        }
    },
    "Edit_File": {
        "type": "function",
        "function": {
            "name": "Edit_File",
            "description": "编辑文件内容。用于编辑现有文件内容。",
            "parameters": {
                "type": "object",
                "properties": {
                    "file_path": {
                        "type": "string",
                        "description": "文件路径，必须使用绝对路径（以/开头）。"
                    },
                    "content": {
                        "type": "map",
                        "description": "要写入的文本内容，key为行号，value为要那一行写入的文本内容。"
                    }
                },
                "required": ["file_path", "content"]
            }
        }
    },
    "Send_Command": {
        "type": "function",
        "function": {
            "name": "Send_Command",
            "description": "在指定的shell会话中执行命令。用于运行代码、安装软件包或管理文件。",
            "parameters": {
                "type": "object",
                "properties": {
                    "command": {
                        "type": "string",
                        "description": "要执行的命令。"
                    },
                    "terminal_id": {
                        "type": "string",
                        "description": "（必填）目标终端的唯一ID。"
                    },
                    "dir": {
                        "type": "string",
                        "description": "执行命令的目录，支持相对路径（相对于~/Lysandra目录）和绝对路径。"
                    }
                },
                "required": ["command", "terminal_id", "dir"]
            }
        }
    },
    "View_Terminal": {
        "type": "function",
        "function": {
            "name": "View_Terminal",
            "description": "查看指定Terminal的输出内容的内容。用于检查命令执行结果或监控输出。",
            "parameters": {
                "type": "object",
                "properties": {
                    "terminal_id": {
                        "type": "string",
                        "description": "（必填）目标终端的唯一ID。"
                    }
                },
                "required": ["terminal_id"]
            }
        }
    }
}