{
    "Read_File": {
        "type": "function",
        "function": {
            "name": "Read_File",
            "description": "读取文件内容。用于查看文件内容、分析日志或读取配置文件。",
            "parameters": {
                "type": "object",
                "properties": {
                    "file_path": {
                        "type": "string",
                        "description": "文件路径，支持相对路径（默认在~/MyManus目录下）和绝对路径。"
                    },
                    "start_line": {
                        "type": "integer",
                        "description": "（可选）起始行号，从1开始。"
                    },
                    "end_line": {
                        "type": "integer",
                        "description": "（可选）结束行号，从1开始。"
                    }
                },
                "required": ["file_path"]
            }
        }
    },
    "Write_File": {
        "type": "function",
        "function": {
            "name": "Write_File",
            "description": "覆盖或追加内容到文件。用于创建新文件、追加内容或修改现有文件。",
            "parameters": {
                "type": "object",
                "properties": {
                    "file_path": {
                        "type": "string",
                        "description": "文件路径，支持相对路径（相对路径默认在~/MyManus目录下）和绝对路径。"
                    },
                    "content": {
                        "type": "string",
                        "description": "要写入的文本内容"
                    },
                    "append": {
                        "type": "boolean",
                        "description": "（可选）是否启用追加模式，默认为false。"
                    },
                    "front_newline": {
                        "type": "boolean",
                        "description": "（可选）是否在写入内容前面添加换行符，默认为false。"
                    },
                    "end_newline": {
                        "type": "boolean",
                        "description": "（可选）是否在写入内容后面添加换行符，默认为false。"
                    }
                },
                "required": ["file_path", "content"]
            }
        }
    },
    "Edit_File": {
        "type": "function",
        "function": {
            "name": "Edit_File",
            "description": "编辑文件内容。用于编辑现有文件内容。",
            "parameters": {
                "type": "object",
                "properties": {
                    "file_path": {
                        "type": "string",
                        "description": "文件路径，支持相对路径（默认在~/MyManus目录下）和绝对路径。"
                    },
                    "content": {
                        "type": "map",
                        "description": "要写入的文本内容，key为行号，value为要那一行写入的文本内容。"
                    }
                },
                "required": ["file_path", "content"]
            }
        }
    }
}